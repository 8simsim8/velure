@import 'nib'

.b-reclame {
	& {
	}
	.wrap {
	}
	&__item {
		& {
			position: relative;
		}
		&:not(:last-child):after {
			position: absolute;
			top: calc(50% - 40px);
			right: 0;
			content: '';
			height: 80px;
			border-left: 1px solid #e5e5e5;
		}
		&-link {
			& {
				position: relative;
				display: block;
				padding: 10px;
				padding-right: 13%;
				background-image: none;
				background-position: right 10px center;
				background-size: 48px;
				border-radius: 6px;
				border: 1px solid #E0E0E0;
				cursor: pointer;
				transition: box-shadow 0.3s ease;

			}
			&:before {
				position: absolute;
				font-size: 28px;
				right: 10px;
				color: #a699a4;
				top: calc(50% - 14px);
				opacity: 0.5;
				transition: opacity 0.3s ease;
			}
			&:hover {
				& {
					box-shadow: 0 0 9px rgba(224,224,224,0.75);
				}
				&:before {
					opacity: 1;
				}
			}
		}
		&-block {
			display: flex;
		}
		&-img {
			& {
				width: 30%;
				height: 100px;
				margin-right: 20px;
			}
			img {
				height: 100%;
				width: 100%;
				object-fit: contain;
			}
		}
		&-text {
			& {
				width : 57%;
				margin-right : 20px;
			}
			.but-text {
				color : #A823A3;
				font-size: 13px;
				text-transform: uppercase;
				margin-bottom: 10px;
			}
			.text {
				color: #40363E;
			}
		}
		&:after {
			content
		}
	}
}


.b-programs {
	& {
		padding : 110px 0 70px;
	}
	.icon-ligature {
		color: #a823a3;
	}
	h2 {
		padding: 0 20px;
		margin-bottom: 1.18em;
		text-align: center;
		color : #000000;
	}
	&__shadow {
		& {
			margin-bottom: 3em;
			text-align: center;
			width : 100%;
			color : #FFFFFF;
		}
		&:before {
			font-size: 340px;
			text-shadow: 4px 16px 19px rgb(220, 220, 220);
			transition: text-shadow 1.5s ease 0.5s;
		}
	}
	&__shadow.start {
		& {
		}
		&:before {
			text-shadow: 4px 0px 19px rgb(220, 220, 220);
		}

	}
	.normal {
